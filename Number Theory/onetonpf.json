"OneToNPrimeFactors": {
    "prefix": "OneToNPrimeFactors",
    "body": [
      "vector<vector<int>> PrimeFactors(mx);",
      "",
      "void PrimeFactorization(int n)",
      "{",
      "    // Can Sieve First",
      "    for (auto p : primes)",
      "    {",
      "        for (int i = p; i <= n; i += p)",
      "        {",
      "            PrimeFactors[i].push_back(p);",
      "        }",
      "    }",
      "",
      "    for (int i = 1; i <= n; i++)",
      "    {",
      "        cout << i << \" : \";",
      "        for (auto u : PrimeFactors[i])",
      "        {",
      "            cout << u << ' ';",
      "        }",
      "        cout << endl;",
      "    }",
      "}"
    ],
    "description": "OneToNPrimeFactors"
  }


















